{
  "name": "samuel-ipsum",
  "version": "1.0.5",
  "description": "Generate placeholder text from Samuel L. Jackson quotes",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run fetch-quotes",
    "test": "jest",
    "fetch-quotes": "node scripts/fetch-quotes.js > quotes.json",
    "prepublish": "npm run fetch-quotes"
  },
  "config": {
    "quotesUrl": "http://slipsum.com/js/slipsum.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marinko-peso/samuel-ipsum.git"
  },
  "bin": {
    "samuel-ipsum": "cli.js",
    "slipsum": "cli.js"
  },
  "keywords": [
    "placeholder",
    "lorem",
    "ipsum",
    "samuel",
    "jackon"
  ],
  "author": "Marinko Peso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marinko-peso/samuel-ipsum/issues"
  },
  "homepage": "https://github.com/marinko-peso/samuel-ipsum#readme",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-semistandard": "^12.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "got": "^8.3.1",
    "jest": "^23.1.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "hr": "^0.1.3",
    "meow": "^5.0.0"
  },
  "files": [
    "index.js",
    "cli.js",
    "quotes.json"
  ]
